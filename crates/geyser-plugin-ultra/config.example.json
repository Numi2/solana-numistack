
{
    "libpath": "/opt/solana/plugins/libgeyser_plugin_ultra.so",
    "socket_path": "/var/run/ultra-geyser.sock",
    "queue_capacity": 65536,
    "batch_max": 256,
    "batch_bytes_max": 2097152,
    "flush_after_ms": 0,
    "pin_core": 2,
    "rt_priority": 80,
    "sched_policy": "fifo",
    "streams": { "accounts": true, "transactions": true, "blocks": true, "slots": true },
    "metrics": { "listen_addr": "0.0.0.0:9977" }
}
