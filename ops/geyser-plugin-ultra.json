
{
  "libpath": "/usr/local/lib/libgeyser_plugin_ultra.so",
  "socket_path": "/var/run/ultra-geyser.sock",
  "queue_capacity": 8192,
  "queue_drop_policy": "drop_newest",
  "batch_max": 64,
  "batch_bytes_max": 1048576,
  "flush_after_ms": 0,
  "write_timeout_ms": 200,
  "write_spin_cap_us": 300,
  "write_sleep_backoff_us": 750,
  "pin_core": 2,
  "rt_priority": 90,
  "sched_policy": "fifo",
  "histogram_sample_log2": 8,
  "streams": {
    "accounts": true,
    "transactions": true,
    "blocks": true,
    "slots": true
  },
  "writer_threads": 4,
  "metrics": { "listen_addr": "0.0.0.0:9977" }
}